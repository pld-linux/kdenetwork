+    if (custom_id) {
+	*clientID = configGroup()->readUnsignedNumEntry("clientID", *clientID);
+	*verMajor = configGroup()->readUnsignedNumEntry("clientVersionMajor",
+		    *verMinor);
+	*verMinor = configGroup()->readUnsignedNumEntry("clientVersionMinor",
+		    *verMinor);
+    }
+
