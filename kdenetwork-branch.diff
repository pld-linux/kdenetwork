+	QString realText = message.text();
+	if ( message.properties() & Oscar::Message::NotDecoded )
+		realText = codec->toUnicode( message.textArray() );
+	
+	
+	if ( !realText.isEmpty() )
+		setProperty( mProtocol->awayMessage, realText );
+	else
+		removeProperty( mProtocol->awayMessage );
