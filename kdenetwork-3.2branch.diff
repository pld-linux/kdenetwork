diff -urN -x CVS kdenetwork.orig/kopete/debian/rules kdenetwork/kopete/debian/rules
--- kdenetwork.orig/kopete/debian/rules	Wed Aug  6 19:40:35 2003
+++ kdenetwork/kopete/debian/rules	Thu Jan  1 01:00:00 1970
@@ -1,59 +0,0 @@
-#!/usr/bin/make -f
-# Made with the aid of debmake, by Christoph Lameter,
-# based on the sample debian/rules file for GNU hello by Ian Jackson.
-
-package=kopete
-
-build:
-	$(checkdir)
-	./configure --prefix=/usr --with-qt-includes=/usr/include/qt3/ --with-qt-libraries=/usr/lib/qt3/
-	$(MAKE) CFLAGS="-O2 -g -Wall"
-	touch build
-
-clean:
-	$(checkdir)
-	rm -f build
-	-$(MAKE) distclean
-	rm -f `find . -name "*~"`
-	rm -rf debian/tmp debian/files* core debian/substvars
-
-binary-indep: checkroot build
-	$(checkdir)
-# There are no architecture-independent files to be uploaded
-# generated by this package.  If there were any they would be
-# made here.
-
-binary-arch: checkroot build
-	$(checkdir)
-	rm -rf debian/tmp
-	install -d debian/tmp
-	cd debian/tmp && install -d `cat ../dirs`
-	$(MAKE) install prefix=`pwd`/debian/tmp/usr
-	# upstream installs some files mode 755 that shouldn't be
-	# we have to change that
-	find `pwd`/debian/tmp \( -name "*.so*" -o -name "*.la" \) -exec chmod -x {} \;
-	# some libs are not stripped, let's take care of that
-	find `pwd`/debian/tmp -name "*.so*" -type f -exec strip --remove-section=.comment --strip-unneeded {} \;
-# Must have debmake installed for this to work. Otherwise please copy
-# /usr/bin/debstd into the debian directory and change debstd to debian/debstd
-	debstd -u ChangeLog README 
-	# Remove xlibs (>> 4.1.0) entry
-	cat debian/substvars
-	#sed 's/, xlibs (>> 4.1.0)//g' < debian/substvars > debian/s.new && mv debian/s.new debian/substvars; done
-	cat debian/substvars
-	dpkg-gencontrol -isp
-	chown -R root.root debian/tmp
-	chmod -R go=rX debian/tmp
-	dpkg --build debian/tmp ..
-
-define checkdir
-	test -f debian/rules
-endef
-
-binary: binary-indep binary-arch
-
-checkroot:
-	$(checkdir)
-	test root = "`whoami`"
-
-.PHONY: binary binary-arch binary-indep clean checkroot
